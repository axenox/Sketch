            "args": {
                "arcType"       : {"type": "choice", "value": "circle", "options": ["circle", "ellipse"], "name": "Arc type"},
                "align"         : {"type": "choice", "value": "horizontal", "options": ["horizontal", "vertical"],
                    "name": "Circle alignment",
                    "depends": {"arcType": "circle"},
                    "description": "Specifies how the circle radius should be calculated depending on the shape width and height"
                },
                "strokeFill": {"name": "Stroke fill", "type": "boolean", "value": false, "description": "Specifies whether this primitive should use the stroke color as its fill"},
                "outline": {"name": "Is Outline", "type": "boolean", "value": false, "description": "Specifies whether this item should be used as part of shape outline"}
            },
            "init": [
                "x = 0; y = 0; w = width; h = height",
                "p1 = Vector(0,0); p2 = Vector(0, 0)",
                "if (args.arcType == 'circle') {",
                "   if (args.align == 'horizontal') {",
                "       h = w",
                "       y = height/2 - h/2",
                "       p1 = Vector(0, height/2)",
                "       p2 = Vector(width, height/2)",
                "   } else {",
                "       w = h",
                "       x = width/2 - w/2",
                "       p1 = Vector(width/2, 0)",
                "       p2 = Vector(width/2, height)",
                "   }",
                "}"
            ],
