{
    "name": "Layered diagram",
    "args": {
        "layers": {"type": "number", "value": 3, "name": "Layers", "min": 1},
        "buttonWidth": {"type": "number", "value": 120, "name": "Button Width", "min": 1},
        "buttonHeight": {"type": "number", "value": 30, "name": "Button Height", "min": 1},
        "buttonGap": {"type": "number", "value": 5, "name": "Button Gap", "min": 0},
        "buttonCornerRadius": {"type": "number", "value": 10, "name": "Button Corner Radius", "min": 0},
        "padding": {"type": "number", "value": 20, "name": "Padding", "min": 0},
        "activeLayer": {"type": "number", "value": 1, "name": "Active layer", "min": 1,
            "description": "Which layer should be displayed on top. Use this property to toggle display of particular layer so that you can draw within that layer"
        },
        "animationDuration": {"type": "number", "value": 0.3, "name": "Animation duration", "min": 0}
    },
    "description": "",
    "preview": "data:image/svg+xml;base64,",
    "init": [
        "deleteLayer = (layerIdx) => {",
        "    for (i = layerIdx; i < layers; i++) {",
        "         moveNativeChildren(`layer-${i+1}`, `layer-${i}`)",
        "         btnNext = findItemByTemplatedId(`btn-${i+1}`)",
        "         btn = findItemByTemplatedId(`btn-${i}`)",
        "         if (btnNext && btn) {",
        "               btn.textSlots = clone(btnNext.textSlots)",
        "         }",
        "    }",
        "    layers = max(1, layers - 1)",
        "}"
    ],
    "controls": [ {
        "name": "addLayer",
        "type": "button",
        "text": "Add layer",
        "placement": "TL",
        "x": {"$-expr": "padding + (buttonWidth + buttonGap) * layers"},
        "y": {"$-expr": "height - buttonHeight - padding"},
        "width": 60,
        "height": 30,
        "click": [ "layers += 1" ]
    }, {
        "$-for": {"start": 1, "until": {"$-expr": "layers + 1"}, "it": "idx"},
        "data": {
            "buttonIndex": {"$-expr": "idx"}
        },
        "name": "deleteLayer",
        "selectedItemId": {"$-str": "btn-${idx}"},
        "type": "button",
        "text": "â¤«",
        "placement": "BL",
        "x": {"$-expr": "padding + (buttonWidth + buttonGap) * idx - buttonGap"},
        "y": {"$-expr": "height - padding - buttonHeight"},
        "width": 20,
        "height": 20,
        "click": [
            "deleteLayer(control.data.buttonIndex)"
        ]
    }, {
        "$-for": {"start": 1, "until": {"$-expr": "layers + 1"}, "it": "idx"},
        "data": {
            "buttonIndex": {"$-expr": "idx"}
        },
        "$-if": "idx != activeLayer",
        "name": "selectLayer",
        "selectedItemId": {"$-str": "btn-${idx}"},
        "type": "button",
        "text": "Toggle layer",
        "placement": "TL",
        "x": {"$-expr": "padding + (buttonWidth + buttonGap) * (idx - 1)"},
        "y": {"$-expr": "height - padding + 5"},
        "width": {"$-expr": "buttonWidth"},
        "height": 30,
        "click": [ "activeLayer = control.data.buttonIndex" ]
    } ],
    "item": {
        "id": "root",
        "name": "Layered diagram",
        "args": {
            "templateIgnoredProps": ["shapeProps.*"]
        },
        "textSlots": {
            "title": {
                "text": "<p>Layered Diagram</p>",
                "bold": true,
                "color": "#969696",
                "fontSize": 16,
                "halign": "center",
                "valign": "middle"
            }
        },
        "shapeProps": {
            "fill": { "type": "none" },
            "strokeSize": 3,
            "strokeColor": "rgba(160,160,160,1.0)",
            "strokePattern": "solid",
            "gap": 308,
            "pos": "top",
            "align": "middle",
            "cornerRadius": 20
        },
        "shape": "uml_group",
        "area": {
            "x": 0,
            "y": 0,
            "w": {"$-expr": "width"},
            "h": {"$-expr": "height"}
        },
        "behavior": {
            "events": []
        },
        "childItems": [ {
            "$-for": {"start": 1, "until": {"$-expr": "layers + 1"}, "it": "idx"},
            "id"   : {"$-str": "btn-${idx}"},
            "name" : {"$-str": "button-${idx}"},
            "args": {
                "templateIgnoredProps": ["shapeProps.*"]
            },
            "area" : {
                "x": {"$-expr": "padding + (buttonWidth + buttonGap) * (idx - 1)"},
                "y": {"$-expr": "height - buttonHeight - padding"},
                "w": {"$-expr": "buttonWidth"},
                "h": {"$-expr": "buttonHeight"}
            },
            "shapeProps": {
                "cornerRadius": {"$-expr": "buttonCornerRadius"},
                "fill": {
                    "type": "solid",
                    "color": "rgba(80, 182, 228, 1)"
                },
                "strokeSize": 0
            },
            "shape": "rect",
            "cursor": "pointer",
            "textSlots": {
                "body": {
                    "text": {"$-str": "<p>Layer ${idx}</p>"},
                    "bold": true,
                    "color": "rgba(255, 255, 255, 1)",
                    "halign": "center",
                    "valign": "middle",
                    "fontSize": 14
                }
            },
            "behavior": {
                "events": [{
                    "id": {"$-expr": "uid()"},
                    "event": "clicked",
                    "actions": [{
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#layer-${idx}"},
                        "method": "condition",
                        "args": {
                            "expression": "isVisible()",
                            "success": "send-event",
                            "successEvent": "hide-layer",
                            "fail": "send-event",
                            "failEvent": "show-layer"
                        }
                    } ]
                }],
                "dragging": "none",
                "dropTo": "",
                "dragPath": "",
                "dragPathAlign": false,
                "dragPathRotation": 0
            }
        }, {
            "$-for": {"start": 1, "until": {"$-expr": "layers + 1"}, "it": "idx"},
            "$-if" : "idx != activeLayer",
            "id"   : {"$-str": "layer-${idx}"},
            "name" : {"$-str": "layer-${idx}"},
            "area" : {
                "x": {"$-expr": "padding"},
                "y": {"$-expr": "padding"},
                "w": {"$-expr": "max(1, width - 2 * padding)"},
                "h": {"$-expr": "max(1, height - buttonHeight - 2 * padding - 5)"}
            },
            "shape": "dummy",
            "shapeProps": {
                "cornerRadius": 0,
                "showName": false,
                "strokeSize": 1
            },
            "behavior": {
                "events": [{
                    "id": {"$-expr": "uid()"},
                    "event": "hide-layer",
                    "actions": [{
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#layer-${idx}"},
                        "method": "hide",
                        "args": {
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": true
                        }
                    }, {
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#btn-${idx}"},
                        "method": "set",
                        "args": {
                            "field": "opacity",
                            "value": 50,
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": false
                        }
                    }]
                }, {
                    "id": {"$-expr": "uid()"},
                    "event": "show-layer",
                    "actions": [{
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#layer-${idx}"},
                        "method": "show",
                        "args": {
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": true
                        }
                    }, {
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#btn-${idx}"},
                        "method": "set",
                        "args": {
                            "field": "opacity",
                            "value": 100,
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": false
                        }
                    }]
                }]
            }
        }, {
            "id"   : {"$-str": "layer-${activeLayer}"},
            "name" : {"$-str": "layer-${activeLayer}"},
            "area" : {
                "x": {"$-expr": "padding"},
                "y": {"$-expr": "padding"},
                "w": {"$-expr": "max(1, width - 2 * padding)"},
                "h": {"$-expr": "max(1, height - buttonHeight - 2 * padding - 5)"}
            },
            "shape": "dummy",
            "shapeProps": {
                "cornerRadius": 0,
                "showName": true,
                "strokeSize": 1
            },
            "behavior": {
                "events": [{
                    "id": {"$-expr": "uid()"},
                    "event": "hide-layer",
                    "actions": [{
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#layer-${activeLayer}"},
                        "method": "hide",
                        "args": {
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": true
                        }
                    }, {
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#btn-${activeLayer}"},
                        "method": "set",
                        "args": {
                            "field": "opacity",
                            "value": 50,
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": false
                        }
                    }]
                }, {
                    "id": {"$-expr": "uid()"},
                    "event": "show-layer",
                    "actions": [{
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#layer-${activeLayer}"},
                        "method": "show",
                        "args": {
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": true
                        }
                    }, {
                        "id": {"$-expr": "uid()"},
                        "element": {"$-str": "#btn-${activeLayer}"},
                        "method": "set",
                        "args": {
                            "field": "opacity",
                            "value": 100,
                            "animated": true,
                            "animationDuration": {"$-expr": "max(0.05, animationDuration)"},
                            "inBackground": false
                        }
                    }]
                }]
            }
        }]
    }
}