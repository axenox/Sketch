{
    "name": "Frame-by-frame diagram",
    "args": {
        "frames": {"type": "number", "value": 3, "name": "Frames", "min": 1},
        "background": {"type": "advanced-color", "value": { "type": "solid", "color": "rgba(240, 240, 240, 1)" }, "name": "Background"},
        "strokeColor": {"type": "color", "value": "rgba(212,212,212,1.0)", "name": "Stroke color"},
        "strokeSize": {"type": "number", "value": 1, "min": 0, "name": "Stroke size"},
        "textColor": {"type": "color", "value": "rgba(30,30,30,1.0)", "name": "Text color"},
        "shadow": {"type": "boolean", "value": true, "name": "Drop shadow"},
        "buttonColor": {"type": "advanced-color", "value": { "type": "solid", "color": "rgba(78, 131, 245, 1)" }, "name": "Button color"},
        "iconColor": {"type": "color", "value": "rgba(255, 255, 255, 1)", "name": "Icon color"},
        "buttonSize": {"type": "number", "value": 60, "min": 5, "name": "Button size"},
        "buttonOpacity": {"type": "number", "value": 60, "min": 5, "name": "Button opacity"},
        "padding": {"type": "number", "value": 14, "min": 0, "name": "Padding"},
        "cornerRadius": {"type": "number", "value": 10, "min": 0, "name": "Corner radius"},
        "frameFill": {"type": "advanced-color", "value": { "type": "solid", "color": "rgba(250, 250, 250, 1)" }, "name": "Frame background"},
        "frameColor": {"type": "color", "value": "rgba(212,212,212,1.0)", "name": "Frame color"},
        "frameStrokeSize": {"type": "number", "value": 1, "min": 0, "name": "Frame stroke size"},
        "animationDuration": {"type": "number", "value": 0.3, "name": "Animation duration", "min": 0}
    },
    "description": "Lets you design simple animated diagram that shows elements frame by frame.\nTo make it work you need to tag your items with \"frame-1\", \"frame-2\", \"frame-3\" and so on for every frame",
    "preview": "data:image/svg+xml;base64,",
    "init": [
        "buttonMargin = round(padding / 2)",
        "containerMarginX = buttonMargin*2 + buttonSize",
        "containerMarginTop = 60",
        "btnIconK = buttonSize / 60",
        "btnIconWidth = 20 * btnIconK",
        "btnIconHeight = 25 * btnIconK",
        "containerMarginBottom = padding",
        "slideWidth = round(max(1, width - containerMarginX * 2))",
        "slideHeight = round(max(1, height - containerMarginTop - containerMarginBottom))",
        "slideMargin = 10",
        "currentSlideVarName = 'current-slide-' + uid()",
        "headerWidth = min(max(200, round(width / 2)), width)"
    ],
    "item": {
        "id": "root",
        "name": "Framed Diagram",
        "shapeProps": {
            "cornerRadius": {"$-expr": "cornerRadius"},
            "fill": {"$-expr": "background"},
            "strokeColor": {"$-expr": "strokeColor"},
            "strokeSize": {"$-expr": "strokeSize"},
            "strokePattern": "solid"
        },
        "shape": "rect",
        "area": { "x": 0, "y": 0, "w": 600, "h": 400, "r": 0, "px": 0.5, "py": 0.5, "sx": 1, "sy": 1 },
        "behavior": {
            "events": [{
                "id": {"$-expr": "uid()"},
                "event": "init",
                "actions": [{
                    "id": {"$-expr": "uid()"},
                    "method": "script",
                    "on": true,
                    "element": "self",
                    "args": {
                        "script": {"$-str": [
                            "setVar('frames', ${frames})",
                            "setVar('currentFrame', 1)",
                            "sendEvent('toggle-current-frame')"
                        ]},
                        "animated": false
                    }
                } ]
            }, {
                "id": {"$-expr": "uid()"},
                "event": "toggle-current-frame",
                "actions": [{
                    "id": {"$-expr": "uid()"},
                    "method": "script",
                    "on": true,
                    "element": "self",
                    "args": {
                        "script": {"$-rawstr": [
                            "frames = getVar('frames')",
                            "currentFrame = getVar('currentFrame')",
                            "for (frame = 1; frame <= currentFrame; frame++) {",
                            "    findChildItemsByTag(`frame-${frame}`).forEach((item) => {item.show()})",
                            "}",
                            "for (frame = currentFrame+1; frame <= frames; frame++) {",
                            "    findChildItemsByTag(`frame-${frame}`).forEach((item) => {item.hide()})",
                            "}",
                            "if (currentFrame == 1) {",
                            "    findChildItemByName('Prev button').hide()",
                            "} else {",
                            "    findChildItemByName('Prev button').show()",
                            "}",
                            "if (currentFrame >= frames) {",
                            "    findChildItemByName('Next button').hide()",
                            "} else {",
                            "    findChildItemByName('Next button').show()",
                            "}"
                        ]},
                        "animated": false
                    }
                } ]
            }, {
                "id": {"$-expr": "uid()"},
                "event": "to-previous-frame",
                "actions": [{
                    "id": {"$-expr": "uid()"},
                    "method": "script",
                    "on": true,
                    "element": "self",
                    "args": {
                        "script": {"$-rawstr": [
                            "currentFrame = getVar('currentFrame')",
                            "if (currentFrame > 1) {",
                            "    currentFrame--",
                            "    setVar('currentFrame', currentFrame)",
                            "    sendEvent('toggle-current-frame')",
                            "}"
                        ]},
                        "animated": false
                    }
                } ]
            }, {
                "id": {"$-expr": "uid()"},
                "event": "to-next-frame",
                "actions": [{
                    "id": {"$-expr": "uid()"},
                    "method": "script",
                    "on": true,
                    "element": "self",
                    "args": {
                        "script": {"$-rawstr": [
                            "frames = getVar('frames')",
                            "currentFrame = getVar('currentFrame')",
                            "if (currentFrame < frames) {",
                            "    currentFrame++",
                            "    setVar('currentFrame', currentFrame)",
                            "    sendEvent('toggle-current-frame')",
                            "}"
                        ]},
                        "animated": false
                    }
                } ]
            }]
        },
        "tags": [],
        "effects": [ {
            "$-if": "shadow == true",
            "id": {"$-expr": "uid()"},
            "effect": "drop-shadow",
            "name": "Drop Shadow",
            "args": {
                "color": "rgba(0,0,0,1.0)",
                "dx": 3,
                "dy": 3,
                "blur": 20,
                "opacity": 20,
                "inside": false
            }
        } ],
        "childItems": [ {
            "id": "header",
            "name": "Header",
            "shape": "none",
            "textSlots": {
                "body": {
                    "text": "<p><strong>Framed diagram</strong></p>",
                    "paddingLeft": 0,
                    "paddingRight": 0,
                    "paddingTop": 0,
                    "paddingBottom": 0,
                    "color": {"$-expr": "textColor"},
                    "halign": "center",
                    "valign": "middle",
                    "fontSize": 20,
                    "whiteSpace": "normal",
                    "font": "Arial"
                }
            },
            "locked": true,
            "area": {
                "x": {"$-expr": "(width - headerWidth)/2"},
                "y": 0,
                "w": {"$-expr": "headerWidth"},
                "h": 40
            },
            "opacity": 100,
            "behavior": {
                "events": []
            },
            "shapeProps": {},
            "selfOpacity": 100,
            "visible": true
        }, {
            "id": "container",
            "name": "Container",
            "shape": "rect",
            "locked": true,
            "shapeProps": {
                "cornerRadius": 0,
                "fill": {"$-expr": "frameFill"},
                "strokeColor": {"$-expr": "frameColor"},
                "strokeSize": {"$-expr": "frameStrokeSize"},
                "strokePattern": "solid"
            },
            "area": {
                "x": {"$-expr": "containerMarginX"},
                "y": {"$-expr": "containerMarginTop"},
                "w": {"$-expr": "slideWidth"},
                "h": {"$-expr": "slideHeight"}
            },
            "opacity": 100,
            "textSlots": { },
            "behavior": {
                "events": []
            },
            "selfOpacity": 100,
            "visible": true
        }, {
            "id": "btn-prev",
            "shape": "ellipse",
            "locked": true,
            "area": {
                "x": {"$-expr": "buttonMargin"},
                "y": {"$-expr": "(height - buttonSize) / 2"},
                "w": {"$-expr": "buttonSize"},
                "h": {"$-expr": "buttonSize"}
            },
            "opacity": {"$-expr": "buttonOpacity"},
            "name": "Prev button",
            "textSlots": { },
            "behavior": {
                "events": [{
                    "id": {"$-expr": "uid()"},
                    "event": "init",
                    "actions": [{
                        "id": {"$-expr": "uid()"},
                        "on": true,
                        "element": "self",
                        "method": "hide",
                        "args": {
                            "animated": false
                        }
                    }]
                } ]
            },
            "shapeProps": {
                "fill": {"$-expr": "buttonColor"},
                "strokeColor": "rgba(30,30,30,1.0)",
                "strokeSize": 0,
                "strokePattern": "solid"
            },
            "selfOpacity": 100,
            "visible": true,
            "tags": [],
            "clip": false,
            "effects": [],
            "childItems": [ {
                "id": "prev-icon",
                "name": "Prev icon",
                "locked": true,
                "area": {
                    "x": {"$-expr": "15 * btnIconK"},
                    "y": {"$-expr": "18 * btnIconK"},
                    "w": {"$-expr": "btnIconWidth"},
                    "h": {"$-expr": "btnIconHeight"},
                    "r": 0, "sx": 1, "sy": 1, "px": 0.5, "py": 0.5
                },
                "shape": "path",
                "shapeProps": {
                    "paths": [
                        {
                            "id": {"$-expr": "uid()"},
                            "closed": true,
                            "points": [
                                { "t": "L", "x": 100, "y": 0 },
                                { "t": "L", "x": 100, "y": 100 },
                                { "t": "L", "x": 0, "y": 50 }
                            ],
                            "pos": "relative"
                        }
                    ],
                    "fill": {
                        "type": "solid",
                        "color": {"$-expr": "iconColor"}
                    },
                    "strokeColor":{"$-expr": "iconColor"},
                    "strokeSize": 4,
                    "strokePattern": "solid"
                },
                "textSlots": {},
                "opacity": 100,
                "selfOpacity": 100,
                "visible": true,
                "tags": [],
                "links": [],
                "blendMode": "normal",
                "cursor": "default",
                "clip": false,
                "effects": [],
                "description": "",
                "behavior": {
                    "events": []
                }
            }, {
                "id": "btn-prev-overlay",
                "name": "Button prev overlay",
                "locked": true,
                "shape": "ellipse",
                "cursor": "pointer",
                "selfOpacity": 0,
                "area": { "x": 0, "y": 0, "w": {"$-expr": "buttonSize"}, "h": {"$-expr": "buttonSize"}, "r": 180, "sx": 1, "sy": 1, "px": 0.5, "py": 0.5 },
                "behavior": {
                    "events": [{
                        "id": {"$-expr": "uid()"},
                        "event": "clicked",
                        "actions": [{
                            "id": {"$-expr": "uid()"},
                            "element": "#root",
                            "method": "sendEvent",
                            "args": {
                                "event": "to-previous-frame"
                            }
                        }]
                    }, {
                        "id": {"$-expr": "uid()"},
                        "event": "mousein",
                        "actions": [{
                            "id": {"$-expr": "uid()"},
                            "element": "#btn-prev",
                            "method": "set",
                            "args": {
                                "field": "opacity",
                                "value": 100,
                                "animated": true
                            }
                        }]
                    }, {
                        "id": {"$-expr": "uid()"},
                        "event": "mouseout",
                        "actions": [{
                            "id": {"$-expr": "uid()"},
                            "element": "#btn-prev",
                            "method": "set",
                            "args": {
                                "field": "opacity",
                                "value": {"$-expr": "buttonOpacity"},
                                "animated": true
                            }
                        }]
                    }]
                }
            } ]
        }, {
            "id": "btn-next",
            "shape": "ellipse",
            "locked": true,
            "cursor": "default",
            "area": {
                "x": {"$-expr": "width - buttonSize - buttonMargin"},
                "y": {"$-expr": "(height - buttonSize) / 2"},
                "w": {"$-expr": "buttonSize"},
                "h": {"$-expr": "buttonSize"},
                "r": 0,
                "px": 0.5,
                "py": 0.5,
                "sx": 1,
                "sy": 1
            },
            "opacity": {"$-expr": "buttonOpacity"},
            "blendMode": "normal",
            "name": "Next button",
            "description": "",
            "links": [],
            "textSlots": { },
            "behavior": {
                "events": []
            },
            "shapeProps": {
                "fill": {"$-expr": "buttonColor"},
                "strokeColor": "rgba(30,30,30,1.0)",
                "strokeSize": 0,
                "strokePattern": "solid"
            },
            "selfOpacity": 100,
            "visible": true,
            "tags": [],
            "clip": false,
            "effects": [],
            "childItems": [ {
                "id": "next-icon",
                "name": "Next icon",
                "locked": true,
                "area": {
                    "x": {"$-expr": "24 * btnIconK"},
                    "y": {"$-expr": "18 * btnIconK"},
                    "w": {"$-expr": "btnIconWidth"},
                    "h": {"$-expr": "btnIconHeight"},
                    "r": 180, "sx": 1, "sy": 1, "px": 0.5, "py": 0.5
                },
                "shape": "path",
                "shapeProps": {
                    "paths": [
                        {
                            "id": {"$-expr": "uid()"},
                            "closed": true,
                            "points": [
                                { "t": "L", "x": 100, "y": 0 },
                                { "t": "L", "x": 100, "y": 100 },
                                { "t": "L", "x": 0, "y": 50 }
                            ],
                            "pos": "relative"
                        }
                    ],
                    "fill": {
                        "type": "solid",
                        "color": {"$-expr": "iconColor"}
                    },
                    "strokeColor": {"$-expr": "iconColor"},
                    "strokeSize": 4,
                    "strokePattern": "solid"
                },
                "textSlots": {},
                "opacity": 100,
                "selfOpacity": 100,
                "visible": true,
                "tags": [],
                "links": [],
                "blendMode": "normal",
                "cursor": "default",
                "clip": false,
                "effects": [],
                "behavior": {
                    "events": []
                }
            }, {
                "id": "btn-next-overlay",
                "name": "Button next overlay",
                "locked": true,
                "shape": "ellipse",
                "cursor": "pointer",
                "selfOpacity": 0,
                "area": { "x": 0, "y": 0, "w": {"$-expr": "buttonSize"}, "h": {"$-expr": "buttonSize"}, "r": 180, "sx": 1, "sy": 1, "px": 0.5, "py": 0.5 },
                "behavior": {
                    "events": [{
                        "id": {"$-expr": "uid()"},
                        "event": "clicked",
                        "actions": [{
                            "id": {"$-expr": "uid()"},
                            "element": "#root",
                            "method": "sendEvent",
                            "args": {
                                "event": "to-next-frame"
                            }
                        }]
                    }, {
                        "id": {"$-expr": "uid()"},
                        "event": "mousein",
                        "actions": [{
                            "id": {"$-expr": "uid()"},
                            "element": "#btn-next",
                            "method": "set",
                            "args": {
                                "field": "opacity",
                                "value": 100,
                                "animated": true
                            }
                        }]
                    }, {
                        "id": {"$-expr": "uid()"},
                        "event": "mouseout",
                        "actions": [{
                            "id": {"$-expr": "uid()"},
                            "element": "#btn-next",
                            "method": "set",
                            "args": {
                                "field": "opacity",
                                "value": {"$-expr": "buttonOpacity"},
                                "animated": true
                            }
                        }]
                    }]
                }
            } ]
        } ],
        "textSlots": { },
        "opacity": 100,
        "selfOpacity": 100,
        "visible": true,
        "blendMode": "normal",
        "cursor": "default",
        "clip": false
    }
}
